{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "lib": [
      "es2017"
    ],
    "removeComments": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "ES5",
    "outDir": "build",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
